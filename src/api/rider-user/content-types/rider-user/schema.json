{
  "kind": "collectionType",
  "collectionName": "rider_users",
  "info": {
    "singularName": "rider-user",
    "pluralName": "rider-users",
    "displayName": "(Rider) User",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user_id": {
      "type": "uid"
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "phone_number": {
      "type": "string"
    },
    "created_date": {
      "type": "datetime"
    },
    "location": {
      "type": "string"
    },
    "workload": {
      "type": "string"
    },
    "earning": {
      "type": "decimal"
    },
    "user_authentcations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rider-user-authentcation.rider-user-authentcation",
      "mappedBy": "user_id"
    },
    "rider_emergency_supports": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rider-emergency-support.rider-emergency-support",
      "mappedBy": "rider_user"
    },
    "rider_order_assignments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rider-order-assignment.rider-order-assignment",
      "mappedBy": "assigned_to"
    },
    "rider_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rider-order.rider-order",
      "mappedBy": "user_id"
    }
  }
}
